<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.quinnnorris.ssm.mapper.ComponentCustomMapper">

    <insert id="insertComponent" useGeneratedKeys="true" keyProperty="com_id" parameterType="ComponentCustom">
        INSERT INTO component (create_id,com_type,com_name,innertest,length,color)
        VALUE (#{create_id},#{com_type},#{com_name},#{innertest},#{length},#{color})
    </insert>

</mapper>